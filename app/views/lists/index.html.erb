<header class="header">
  <h1>todos</h1>
  <%= form_for @list do |f| %>
    <%= f.text_field :name, class: "new-todo", placeholder: "Create a new list!", autofocus: true %>
  <% end %>  
</header>
<section class="main">
  <label for="toggle-all">Mark all as complete</label>
  <ul class="todo-list">
    <% @lists.each do |list| %>
    <li>
      <div class="view">
        <label><%= link_to list.name, list %></label>
      </div>
    </li>
    <% end %>
  </ul>
</section>
<footer class="footer">
</footer>

 